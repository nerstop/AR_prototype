<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vind.android.ar">

    <uses-sdk
        android:minSdkVersion="18"
        android:maxSdkVersion="23"
        android:targetSdkVersion="23"/>

    <application
        android:name=".CustomStartApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.vind.android.ar.activity.LoginActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.vind.android.ar.activity.JoinActivity"
            android:screenOrientation="portrait"/>

        <activity android:name="com.vind.android.ar.activity.MainActivity"
            android:screenOrientation="portrait"/>

        <activity android:name="com.vind.android.ar.activity.WorkoutActivity"
            android:screenOrientation="portrait"/>

        <activity android:name="com.vind.android.ar.activity.PeopleActivity"
            android:screenOrientation="portrait"/>

        <activity android:name="com.vind.android.ar.activity.RankActivity"
            android:screenOrientation="portrait"/>

        <activity android:name="com.vind.android.ar.activity.ProgramActivity"
            android:screenOrientation="portrait"/>

        <activity android:name="com.vind.android.ar.activity.GiftActivity"
            android:screenOrientation="portrait"/>

        <activity android:name="com.vind.android.ar.activity.MoreActivity"
            android:screenOrientation="portrait"/>


    </application>

</manifest>